services:
  - type: web
    name: telegram-bot
    env: python
    plan: free
    buildCommand: "pip install -r requirements.txt"
    startCommand: "python bot.py"
    envVars:
      - key: BOT_TOKEN
        sync: false
      - key: ADMIN_ID
        sync: false
      - key: MAIN_BOT_USERNAME
        sync: false
      - key: UPLOAD_CHANNEL
        sync: false
    pythonVersion: 3.11
